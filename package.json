{
  "name": "syntactor",
  "version": "1.2.0",
  "homepage": "https://gregoor.github.io/syntactor",
  "license": "MIT",
  "private": false,
  "main": "lib/api",
  "repository": {
    "type": "git",
    "url": "https://github.com/Gregoor/syntactor.git"
  },
  "dependencies": {
    "babel-generator": "^6.26.0",
    "babylon": "^6.18.0",
    "immutable": "4.0.0-rc.9",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "styled-components": "^2.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-react-app": "^3.1.0",
    "flow-bin": "^0.62.0",
    "gh-pages": "1.1.0",
    "hubup": "^1.0.1",
    "jest-immutable-matchers": "^1.4.0",
    "react-scripts": "^1.0.17"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build-lib": "babel --out-dir lib/ --ignore test.js src/",
    "build-prod": "NODE_ENV=production yarn build-lib && yarn build",
    "test": "yarn flow && react-scripts test --env=jsdom",
    "flow": "flow",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build",
    "release": "yarn build-prod && hubup $(find build/static/js/ -iname '*.js') '{\"name\":\"syntactor.min.js\"}'"
  }
}
